        <script src="js/bootstrap-multiselect.js" type="text/javascript"></script>


        <script>
            BuscarCentralMarca();

            $('#cbxMarcaCentral').click(function (event) {
                var marca_central = $('#cbxMarcaCentral').val();
                BuscarCentralModelo(marca_central);
            });

            $('#cbxModeloCentral').click(function (event) {
                var modelo_central = $('#cbxModeloCentral').val();
                BuscarDepartamentos(modelo_central);
            });

            $('#cbxDepartamento').click(function (event) {
                var departamento = $('#cbxDepartamento').val();
                BuscarExtensiones(departamento);
            });

            $('#btnBuscar').click(function (event) {

                var selected = $("#cbxExtensiones option:selected");
                var concat = "";
                var extensiones = "";

                selected.each(function () {
                    concat += "extension.ext_id = \\'" + $(this).val() + "\\' OR ";
                });

                extensiones = concat.substring(0, (concat.length - 3));

                BuscarAsignacionLlamads(extensiones);
            });

            function BuscarCentralMarca() {

                $.ajax({
                    type: "GET",
                    url: "/BuscarMarcaCentralConsultasCrm",
                    success: function (responseText) {
                        var htmlOut = '';
                        for (var i = 0; i < responseText.length; i++) {
                            htmlOut += "<option value=\"" + responseText[i].mar_id + "\">" + responseText[i].mar_nombre + "</option>\n"
                        }
                        $('#cbxModeloCentral').empty();
                        $('#cbxDepartamento').empty();
                        $('#cbxExtensiones').empty();
                        $('#cbxExtensiones').multiselect('destroy');

                        $('#cbxMarcaCentral').empty();
                        $('#cbxMarcaCentral').append(htmlOut);
                    }
                });
            }

            function BuscarCentralModelo(marca_central) {

                $.ajax({
                    type: "GET",
                    url: "/BuscarModeloCentralConsultasCrm",
                    data: { marca_central: marca_central },
                    success: function (responseText) {
                        var htmlOut = '';
                        for (var i = 0; i < responseText.length; i++) {
                            htmlOut += "<option value=\"" + responseText[i].cen_id + "\">" + responseText[i].cen_nombre + "</option>\n"
                        }
                        $('#cbxExtensiones').empty();
                        $('#cbxExtensiones').multiselect('destroy');
                        $('#cbxDepartamento').empty();

                        $('#cbxModeloCentral').empty();
                        $('#cbxModeloCentral').append(htmlOut);
                    }
                });
            }

            function BuscarDepartamentos(modelo_central) {

                $.ajax({
                    type: "GET",
                    url: "/BuscarDepartamentoConsultasCrm",
                    data: { modelo_central: modelo_central },
                    success: function (responseText) {
                        var htmlOut = '';
                        for (var i = 0; i < responseText.length; i++) {
                            htmlOut += "<option value=\"" + responseText[i].dep_id + "\">" + responseText[i].dep_nombre + "</option>\n"
                        }
                        $('#cbxExtensiones').empty();
                        $('#cbxExtensiones').multiselect('destroy');

                        $('#cbxDepartamento').empty();
                        $('#cbxDepartamento').append(htmlOut);
                    }
                });
            }

            function BuscarExtensiones(departamento) {
                $.ajax({
                    type: "GET",
                    url: "/BuscarExtensionConsultasCrm",
                    data: { departamento: departamento },
                    success: function (responseText) {
                        var htmlOut = '';
                        for (var i = 0; i < responseText.length; i++) {
                            htmlOut += "<option value='" + responseText[i].ext_id + "'>" + responseText[i].ext_nombre + "  " + responseText[i].ext_numero + "</option>\n";
                        }

                        $('#cbxExtensiones').empty();
                        $('#cbxExtensiones').append(htmlOut);

                        $('#cbxExtensiones').multiselect('destroy');
                        $('#cbxExtensiones').multiselect({
                            includeSelectAllOption: true,
                            enableFiltering: true,
                            buttonWidth: '100%',
                            maxHeight: 200
                        });
                    }
                });
            }

            function BuscarAsignacionLlamads(extensiones) {
                var l = $('.ladda-button').ladda();
                l.ladda('start');

                $.ajax({
                    type: "GET",
                    url: "/BuscarLlamadasAsignacionConsultasCrm",
                    data: { extensiones: extensiones },
                    success: function (responseText) {

                        var table = $('#tbDetalles').DataTable();
                        table.destroy();
                        $('#tbDetalles').empty();

                        table = $('#tbDetalles').DataTable({
                            "ordering": false,
                            pageLength: 10,
                            responsive: true,
                            dom: '<"html5buttons"B>lTfgitp',
                            buttons: [
                                { extend: 'csv', className: 'exportar-csv-e' },
                                { extend: 'excel', title: 'asignacion-search-excel', className: 'exportar-excel-e' },
                                {
                                    text: '<i class=\'fa fa-refresh\'><i>',
                                    action: function (e, dt, node, config) {
                                        location.reload();
                                    }
                                }
                            ],
                            data: responseText[0],
                            columns: [
                                { data: 'ext_nombre', title: "Extensión" },
                                { data: 'departamento', title: "Departamento" },
                                { data: 'asig_apellidos', title: "A. Apellido" },
                                { data: 'asig_nombre', title: "A. Nombre" },
                                { data: 'asig_direccion', title: "A. Dirección" },
                                { data: 'asig_poblacion', title: "A. Población" },
                                { data: 'asig_telefono', title: "A. Teléfono" },
                                { data: 'asig_email', title: "A. Email" },
                                { data: 'asig_genero', title: "A. Género" },
                                { data: 'asig_ingresos', title: "A. Ingresos" },
                                { data: 'asig_descripcion', title: "A. Descripción" },
                                { data: 'asig_comentario', title: "A. Comentario" },
                                { data: 'asig_estado', title: "A. Estado" }
                            ],
                            "columnDefs": [
                                { "visible": false, "targets": 0 },
                                {
                                    "targets": [1, 4, 5, 7, 8, 9],
                                    "visible": false,
                                    "searchable": false
                                }],
                            "order": [[0, 'asc']],
                            "drawCallback": function (settings) {
                                var api = this.api();
                                var rows = api.rows({ page: 'current' }).nodes();
                                var last = null;

                                api.column(0, { page: 'current' }).data().each(function (group, i) {
                                    if (last !== group) {
                                        $(rows).eq(i).before(
                                            '<tr class="group" style="background: #1AB394; color: white"><td colspan="10"><i class="fa fa-user"></i> <b>Extensión - ' + group + '</b></td></tr>'
                                        );

                                        last = group;
                                    }
                                });
                            },
                            "language": {
                                "sProcessing": "Procesando...",
                                "sLengthMenu": "Mostrar _MENU_ registros",
                                "sZeroRecords": "No se encontraron resultados",
                                "sEmptyTable": "Ningún dato disponible en esta tabla",
                                "sInfo": "Mostrando registros del _START_ al _END_ de un total de _TOTAL_ registros",
                                "sInfoEmpty": "Mostrando registros del 0 al 0 de un total de 0 registros",
                                "sInfoFiltered": "(filtrado de un total de _MAX_ registros)",
                                "sInfoPostFix": "",
                                "sSearch": "",
                                "sUrl": "",
                                "sInfoThousands": ",",
                                "sLoadingRecords": "Cargando...",
                                "oPaginate": {
                                    "sFirst": "Primero",
                                    "sLast": "Último",
                                    "sNext": "Siguiente",
                                    "sPrevious": "Anterior"
                                },
                                "oAria": {
                                    "sSortAscending": ": Activar para ordenar la columna de manera ascendente",
                                    "sSortDescending": ": Activar para ordenar la columna de manera descendente"
                                },
                                searchPlaceholder: "Buscar datos"
                            }
                        });

                        l.ladda('stop');

                        $('#mdBusquedas').modal('toggle');
                    }
                });
            }
        </script>

<script src="js/plugins/sweetalert/sweetalert.min.js"></script>
<script type="text/javascript" src="/socket.io/socket.io.js"></script>

<script src="js/timeOut/timeOut.js"></script>
        <%  } else{ %>
            <script>
                location.replace('/inicio');
            </script>
