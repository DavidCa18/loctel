        <script>
            $(document).ready(function () {
                $('.slick_demo_1').slick({
                    dots: true
                });

                // Instance the tour
                var tour = new Tour({
                    steps: [
                        {
                            element: "#step1",
                            title: "Información",
                            content: "",
                            placement: "top",
                            backdrop: true,
                            backdropContainer: '#wrapper',
                            onShown: function (tour) {
                                $('body').addClass('tour-open')
                            },
                            onHidden: function (tour) {
                                $('body').removeClass('tour-close')
                            }
                        },
                        {
                            element: "#step2",
                            title: "Campos definidos",
                            content: "Los campos definidos para la subida de archivos de llamadas son los siguientes.",
                            placement: "top",
                            backdrop: true,
                            backdropContainer: '#wrapper',
                            onShown: function (tour) {
                                $('body').addClass('tour-open')
                            },
                            onHidden: function (tour) {
                                $('body').removeClass('tour-close')
                            }
                        },
                        {
                            element: "#step3",
                            title: "Tipo Documento",
                            content: "Tipo del documento de llamadas.",
                            placement: "top",
                            backdrop: true,
                            backdropContainer: '#wrapper',
                            onShown: function (tour) {
                                $('body').addClass('tour-open')
                            },
                            onHidden: function (tour) {
                                $('body').removeClass('tour-close')
                            }
                        },
                        {
                            element: "#step4",
                            title: "Formato Documento",
                            content: "Orden de los campos en el archivo <b>.csv</b>",
                            placement: "top",
                            backdrop: true,
                            backdropContainer: '#wrapper',
                            onShown: function (tour) {
                                $('body').addClass('tour-open')
                            },
                            onHidden: function (tour) {
                                $('body').removeClass('tour-close')
                            }
                        },
                        {
                            element: "#step5",
                            title: "Proceso Asignación - Guardado",
                            content: "Pasos a seguir para asignar agentes y guardar las llamadas",
                            placement: "top",
                            backdrop: true,
                            backdropContainer: '#wrapper',
                            onShown: function (tour) {
                                $('body').addClass('tour-open')
                            },
                            onHidden: function (tour) {
                                $('body').removeClass('tour-close')
                            }
                        },
                    ]
                });

                // Initialize the tour
                //tour.init();

                $('.startTour').click(function () {
                    tour.restart();

                    // Start the tour
                    // tour.start();
                })

            });
        </script>

<script src="js/plugins/sweetalert/sweetalert.min.js"></script>
<script type="text/javascript" src="/socket.io/socket.io.js"></script>

<script src="js/timeOut/timeOut.js"></script>
        <%  } else{ %>
            <script>
                location.replace('/inicio');
            </script>
